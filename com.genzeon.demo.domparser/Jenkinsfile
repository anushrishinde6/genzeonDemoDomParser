    agent any
    stages {
        stage('build') {
            steps {
                
                echo 'build stage'
                git 'https://github.com/anushrishinde6/genzeonDemoDomParser.git'
            }
        }
    
     stage('test') {
            
            steps {
                echo 'test stage'
                script 
                {
                  
    			   dir("C:\\Users\\AShinde\\git\\genzeonDemoDomParser\\com.genzeon.demo.domparser") {
    			
    		     	if (isUnix()) {
      		         sh "ant main"
    			    } else {
      			     bat "ant main"
      				}
    			}
    		}
         
     }
  }
        
    stage('deployment') {
            
            steps {
                echo 'deployment stage'
            }
        } 
     }
 }   