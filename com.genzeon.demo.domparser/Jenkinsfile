pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                
                echo 'build stage'
                git 'https://github.com/anushrishinde6/genzeonDemoDomParser.git'
            }
        }
    
     stage('test') {
            
            steps {
                echo 'test stage'
                script 
                {
                   withAnt(installation: 'myinstall') {
    			   dir("scoring") {
    			
    		     	if (isUnix()) {
      		         sh "ant mytarget"
    			    } else {
      			     bat "ant main"
      			}
    			}
}
            }
        }
        
    stage('deployment') {
            
            steps {
                echo 'deployment stage'
            }
        } 
     }
 }   